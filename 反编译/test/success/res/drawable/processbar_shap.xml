<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ff808080" android:endColor="#ff808080" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff808080" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ff00897b" android:endColor="#ff00897b" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff00897b" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ffff2525" android:endColor="#ffff2525" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffff2525" />
            </shape>
        </clip>
    </item>
</layer-list>